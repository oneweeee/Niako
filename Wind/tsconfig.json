{
  "compilerOptions": {
    "target": "ES2020",
    "outDir": "dist",
    "rootDir": "src",
    "module": "CommonJS",
    "strict": true,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "paths": {
      "#client": [ "./struct/WindClient.ts" ],
      "#config": [ "./config.ts" ],
      "#types/*": [ "./types/*" ],
      "#base/*": [ "./struct/base/*" ],
      "#db/*": [ "./db/*" ]
    }
  }
}